generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ContactMessage {
  id       String   @id @default(uuid())
  name     String
  fullName String
  email    String
  reason   String
  content  String
  dateSent DateTime @default(now())
}

enum Role {
  BASIC
  ADMIN
  SUPER_ADMIN
}
